{
  "name": "react-seed",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts-ts": "2.17.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "lint": "yarn tslint \"./src/**/*.ts ./src/**/*.tsx\"",
    "precommit": "concurrently --prefix \"[{name}]\" --names \"TSC,LINT\" -c \"bgBlue.bold,bgMagenta.bold\" \"tsc --noEmit --pretty\" \"yarn tslint ./src/**/*.ts ./src/**/*.tsx\""
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "concurrently": "^3.6.1",
    "husky": "^0.14.3",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.0.1"
  }
}
